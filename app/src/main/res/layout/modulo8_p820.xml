        android:id="@+id/mod8_820_checkbox_C8_P820_10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/modulo_8_p820_sp10"/>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <CheckBox
            android:id="@+id/mod8_820_checkbox_C8_P820_11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/margen_small"
            android:text="@string/modulo_8_p820_sp11"/>
        <EditText
            android:id="@+id/mod8_820_edittext_C8_P820_O"
            style="@style/TemaInputEspecifique"/>
    </LinearLayout>
</LinearLayout>