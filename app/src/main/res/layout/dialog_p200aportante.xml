<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

            <LinearLayout
                android:id="@+id/dialog_p200aportante_ingresos"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:padding="10dp">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:gravity="center"
                    android:textColor="@color/colorPrimaryTransparente"
                    android:textSize="@dimen/margen_large"
                    android:textStyle="bold"
                    android:text="CONSULTA A HOGAR SIN PERCEPCIÃ“N  DE INGRESOS"/>

                <android.support.v7.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:cardBackgroundColor="@color/greyBluePrimaryDark">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="30dp"
                        android:gravity="center"
                        android:textColor="@color/colorIcons"
                        android:textStyle="bold"
                        android:text="HOGAR MIXTO"/>
                </android.support.v7.widget.CardView>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_margin="@dimen/margen_small"
                    android:padding="@dimen/margen_small"
                    android:background="@drawable/marco_pregunta"
                    android:orientation="vertical">

                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/colorPrimary">
                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.5"
                            android:gravity="center"
                            android:theme="@style/TemaCabeceraP309"
                            android:text="@string/modulo_2_cab_nro"/>
                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="2"
                            android:gravity="center"
                            android:theme="@style/TemaCabeceraP309"
                            android:text="@string/modulo_2_cab_nombre"/>
                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="2"
                            android:gravity="center"
                            android:theme="@style/TemaCabeceraP309"
                            android:text="@string/modulo_2_cab_parentesco"/>
                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:theme="@style/TemaCabeceraP309"
                            android:text="@string/modulo_2_cab_sexo"/>
                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:theme="@style/TemaCabeceraP309"
                            android:text="@string/modulo_2_cab_edad"/>
                        <TextView
                            android:visibility="gone"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="2"
                            android:gravity="center"
                            android:theme="@style/TemaCabeceraP309"
                            android:text="@string/modulo_2_cab_estado_civil"/>
                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1.4"
                            android:gravity="center"
                            android:theme="@style/TemaCabeceraP309"
                            android:text="@string/modulo_2_cab_llego_venezuela"/>
                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="4"
                            android:gravity="center"
                            android:theme="@style/TemaCabeceraP309"
                            android:text="Tiene Ingresos?"/>

                    </LinearLayout>
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">
                        <android.support.v7.widget.RecyclerView
                            android:id="@+id/hogarmixto_recyclerview"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"/>
                    </RelativeLayout>
                </LinearLayout>
            </LinearLayout>
</LinearLayout>




